<h2>Sign in</h2>
<%= form_for(resource, :as => resource_name, :url => session_path(resource_name), :html => { :class => "form-horizontal", :id => "contact-form"} ) do |f| %>

	<%= f.email_field :email %>
  <div><%= f.label :password %><br />
  <%= f.password_field :password %></div>
  <div><%= f.submit "Sign in" %></div>

<% end %>
<%= render "devise/shared/links" %>
